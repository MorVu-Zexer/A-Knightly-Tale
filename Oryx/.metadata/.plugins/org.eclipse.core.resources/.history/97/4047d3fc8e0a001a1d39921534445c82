
public class Deck {

	Card[] card = null;
	
	Deck(){
		
	}
	
	public void assignCards() {
		for(int i = 0; i < 4; i++) {
			for (int j = 0; j < 13; j++) {
				if (i == 0) card[(i*13)+j].setSuit("Spades");
				if (i == 0) card[(i*13)+j].setSuit("Hearts");
				if (i == 0) card[(i*13)+j].setSuit("Clubs");
				if (i == 0) card[(i*13)+j].setSuit("Diamonds");
//				
			}
		}
		
		for (int i = 0; i < card.length; i++) {
//			card[i].print();
			System.out.println("Card " + (i+1) + ": ");
		}
	}
	
}

